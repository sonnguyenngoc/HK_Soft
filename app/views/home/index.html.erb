        preload: 0,
        autoplay: true
    });
    (function($) {
        "use strict";
        $('.accordion, .tabs').TabsAccordion({
            hashWatch: true,
            pauseMedia: true,
            responsiveSwitch: 'tablist',
            saveState: sessionStorage,
        });

    })(jQuery);
    $('#slider').loopSlider({
        autoMove: true,
        mouseOnStop: true,
        turn: 9000,
        motion: 'swing',
