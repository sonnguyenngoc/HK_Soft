<div class="pageWrapper">
    
    <!-- Top bar start -->
    <%= render "/home/top_bar" %>
    <!-- Top bar end -->
    
    <!-- Header start -->
    <header class="top-head minimal">
        <div class="container">
            
            <!-- Logo start -->
            <%= render "/home/logo" %>
            <!-- Logo end -->

            <div class="responsive-nav">
                
                <!-- top navigation menu start -->
                <%= render "/home/top_navigation_menu" %>
                <!-- top navigation menu end -->
                
                <div class="f-right">
                    <!-- top search start -->
                    <%= render "/home/top_search" %>
                    <!-- top search end -->
                </div>
                
            </div>
        </div>
    </header>
    <!-- Header start -->

    <!-- Content start -->
    <div class="pageContent">

        <div class="page-title lg-padding">
            <div class="container">
                <h1 class="t-center font-50 uppercase bolder">Welcome To <span class="main-color">Global Naturesoft</span></h1>
                <h3 class="t-center">Now you can check all our work and work in progress, it is amazing and Awesome.</h3>
            </div>
        </div>

        <div class="md-padding">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="filter-by style4">
                            <ul id="filters">
                                <li class="selected"><a href="#" class="filter" data-filter="*"><span>All</span></a></li>
                                <li><a href="#" class="filter" data-filter=".design"><span>Web Design</span></a></li>
                                <li><a href="#" class="filter" data-filter=".develop"><span>Web Development</span></a></li>
                                <li><a href="#" class="filter" data-filter=".computers"><span>Computers</span></a></li>
                            </ul>
                        </div>

                        <div class="portfolio masonry p-3-cols p-style3" id="masonry">
                            <% (1..6).each do |item| %>
                            <div class="portfolio-item design">
                                <figure>
                                    <img alt="" src="/frontend/assets/images/portfolio/grid/1.jpg">
                                    <figcaption>
                                        <div class="port-captions">
                                            <h4><%= link_to "Quality Products for Companies", portfolio_detail_path %></h4>
                                            <p class="description"><a href="#">Design</a>, <a href="#">Development</a></p>
                                        </div>
                                        <div class="icon-links">
                                            <%= link_to '<i class="fa fa-link"></i>'.html_safe, portfolio_detail_path, class: "link" %>
                                            <a href="/frontend/assets/images/portfolio/grid/1.jpg" class="zoom" title="Quality Products for Companies"><i class="fa fa-search-plus"></i></a>
                                        </div>
                                    </figcaption>			
                                </figure>
                            </div>
                            <% end %>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="md-padding">
            <div class="container">

                <div class="heading style2">
                    <h4 class="uppercase">Some Of <span class="main-color">Our Clients</span></h4>
                </div>

                <div class="horizontal-slider" data-slides_count="6" data-scroll_amount="6" data-slider-speed="300" data-slider-infinite="1" data-slider-dots="1" data-slider-arrows="0">
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/1.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/2.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/3.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/4.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/5.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/6.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/7.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/8.png"></a>
                    </div>
                    <div>
                        <a href="#"><img alt="" src="frontend/assets/images/clients/9.png"></a>
                    </div>
                </div>

            </div>
        </div>

    </div>
    <!-- Content start -->

    <!-- Footer start -->
    <%= render "/home/footer" %>
    <!-- Footer end -->

</div>

<!-- Back to top Link -->
<a id="to-top" href="#"><span class="fa fa fa-angle-up"></span></a>

<%= render "/home/js_files" %>

<!-- SLIDER REVOLUTION  -->
<script type="text/javascript" src="/frontend/assets/revolution/js/jquery.themepunch.tools.min.js"></script>
<script type="text/javascript" src="/frontend/assets/revolution/js/jquery.themepunch.revolution.min.js"></script>

<script type="text/javascript">
    var tpj = jQuery;
    var revapi429;
    tpj(window).load(function() {
