                        <div class="col-sms-6 col-sm-6 col-md-3">
                            <article class="box">
                                <% if item.is_sale == true %><span class="discount"><span class="discount-text">Giảm giá <%= item.discount_percent %>%</span></span><% end %>
                                <figure class="animated fadeInDown" data-animation-type="fadeInDown" data-animation-duration="2" style="animation-duration: 2s; visibility: visible;">
                                    <%= link_to image_tag(item.image_url.large_image, width: "270", height: "160"), tour_detail_path(tour_id: item, tour_name: item.url_friendly), class: "hover-effect" %>
                                </figure>
                                <div class="details">
                                    <%= link_to "Xem Tour", tour_detail_path(tour_id: item, tour_name: item.url_friendly), class: "button btn-mini" %>
